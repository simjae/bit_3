package Main;

import study.Emp;

public class Array {

	public static void main(String[] args) {
	//사원 1명을 출력하세요
	Emp emp =new Emp(26,"심재형","IT");
	System.out.println(emp);
	
	//배열을 이용해서 사원 2명을 출력하세요
	Emp[] emplist = {new Emp(27, "박범", "It"), new Emp(30, "전선규", "IT")};
	System.out.println(emp.getEmage());
	
	

	}

}
